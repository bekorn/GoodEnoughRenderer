      "include_paths": [
        "glsl/program/envmap/common.glsl",
      ]
    },
    {
      "name": "envmap_diffuse",
      "stages": {
        "vert": "glsl/program/screen_pass.vert.glsl",
        "frag": "glsl/program/envmap/diffuse.frag.glsl"
      },
      "include_paths": [
        "glsl/program/envmap/common.glsl",
      ]
    },
    {
      "name": "envmap_specular",
      "stages": {
        "vert": "glsl/program/screen_pass.vert.glsl",
        "frag": "glsl/program/envmap/specular.frag.glsl"
      },
      "include_paths":  [
        "glsl/program/envmap/common.glsl",
      ]
    },
  ]
